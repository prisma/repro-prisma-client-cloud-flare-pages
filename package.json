{
  "packageManager": "pnpm@7.28.0",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .ts,.tsx --fix",
    "build": "remix build",
    "dev:remix": "remix watch",
    "dev:wrangler": "wrangler --compatibility-date 2023-03-02 pages dev ./public",
    "dev": "npm-run-all build --print-label --parallel \"dev:*\"",
    "start": "npm run wrangler",
    "check:format": "prettier --check .",
    "check:lint": "eslint . --ext .ts,.tsx --max-warnings 0",
    "check:types": "tsc --noEmit",
    "prisma:development": "dotenv -c development -- prisma",
    "prisma:production": "dotenv -c production -- prisma"
  },
  "dependencies": {
    "@prisma/client": "^4.11.0",
    "@remix-run/cloudflare": "^1.14.0",
    "@remix-run/cloudflare-pages": "^1.14.0",
    "@remix-run/react": "^1.14.0",
    "isbot": "^3.6.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20230228.0",
    "@prisma-labs/prettier-config": "^0.1.0",
    "@remix-run/dev": "^1.14.0",
    "@remix-run/eslint-config": "^1.14.0",
    "@swc/core": "^1.3.37",
    "@tsconfig/node16-strictest": "^1.0.4",
    "@tsconfig/recommended": "^1.0.2",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "dotenv-cli": "^7.0.0",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-deprecation": "^1.3.3",
    "eslint-plugin-only-warn": "^1.1.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-tsdoc": "^0.2.17",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.4",
    "prisma": "^4.11.0",
    "typescript": "^4.9.5",
    "vitest": "^0.29.2",
    "wrangler": "^2.12.0"
  },
  "engines": {
    "node": ">=16.13"
  }
}
